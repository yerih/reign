<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainFragment">


    <fragment
        android:id="@+id/mainFragment"
        android:name="com.admissions.empty_project.presentation.main.MainFragment"
        tools:layout="@layout/fragment_main"
        android:label="MainFragment" >

        <action
            android:id="@+id/action_mainFragment_to_detailFragment2"
            app:destination="@id/detailFragment" />
    </fragment>
    <fragment
        tools:layout="@layout/fragment_detail"
        android:id="@+id/detailFragment"
        android:name="com.admissions.empty_project.presentation.detail.DetailFragment"
        android:label="DetailFragment" >
        <argument
            android:name="url"
            app:argType="string" />
    </fragment>
</navigation>